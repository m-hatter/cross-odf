            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/cross-odf/ontology#CrowdBasedApplication"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/cross-odf/ontology#definesTaskType"/>
                        <owl:someValuesFrom>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://purl.org/cross-odf/ontology#definesProperty"/>
                                <owl:someValuesFrom rdf:resource="http://purl.org/cross-odf/ontology#SpatialProperty"/>
                            </owl:Restriction>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    


