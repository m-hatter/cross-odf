    <owl:Class rdf:about="http://purl.org/cross-odf/ontology#CrowdBasedApplication"/>
    


    <!-- http://purl.org/cross-odf/ontology#CrowdcreationApplication -->

    <owl:Class rdf:about="http://purl.org/cross-odf/ontology#CrowdcreationApplication">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/cross-odf/ontology#CrowdBasedApplication"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/cross-odf/ontology#definesContributionType"/>
                        <owl:someValuesFrom>
                            <owl:Class>
                                <owl:intersectionOf rdf:parseType="Collection">
                                    <rdf:Description rdf:about="http://purl.org/cross-odf/ontology#FreeformTextContribution"/>
                                    <rdf:Description rdf:about="http://purl.org/cross-odf/ontology#FreewillContribution"/>
                                </owl:intersectionOf>
                            </owl:Class>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    


    <!-- http://purl.org/cross-odf/ontology#CrowdfundingApplication -->

    <owl:Class rdf:about="http://purl.org/cross-odf/ontology#CrowdfundingApplication">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/cross-odf/ontology#CrowdBasedApplication"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/cross-odf/ontology#definesContributionType"/>
                        <owl:someValuesFrom rdf:resource="http://purl.org/cross-odf/ontology#MonetaryContribution"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    


    <!-- http://purl.org/cross-odf/ontology#CrowdsensingApplication -->

    <owl:Class rdf:about="http://purl.org/cross-odf/ontology#CrowdsensingApplication">
